{
    "name": "ghazanfarmir/laravel-companies-house",
    "description": "This the Laravel Service Provider package to access CompaniesHouse API within your Laravel application.",
    "keywords": ["companies-house", "laravel"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ghazanfar Mir",
            "email": "GhazanfarMir@gmail.com"
        }
    ],
    "require": {
        "php": "^7.3",
        "illuminate/support": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "GhazanfarMir\\CompaniesHouse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GhazanfarMir\\CompaniesHouse\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "GhazanfarMir\\CompaniesHouse\\CompaniesHouseServiceProvider"
            ],
            "aliases": {
                "CompaniesHouse": "GhazanfarMir\\CompaniesHouse\\Facades\\CompaniesHouse"
            }
        }
    }
}
